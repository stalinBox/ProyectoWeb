	</application>
	<lifecycle>
		<phase-listener>com.project.utils.AuthorizationListener</phase-listener>
	</lifecycle>
	<validator>
		<validator-id>MasMenos10</validator-id>
		<validator-class>com.project.utils.MiValidador</validator-class>
	</validator>
</faces-config>
